#include <bits/stdc++.h>

using namespace std;

int main()
{
    int x,p=0,n=0,z=0;
    int arr[100];
    cin>>x;
    for(int i=0;i<x;i++)
    {
        cin>>arr[i];
    }
    for (int i=0;i<x;i++)
    {
        if(arr[i]>0)
        {
            p++;
        }
        else if(arr[i]<0)
        {
            n++;
        }
        else
        {
            z++;
        }
    }
    cout<<(float)p/x<<endl<<(float)n/x<<endl<<(float)z/x;
    return 0;
}
